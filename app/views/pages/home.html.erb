<%= render 'shared/banner' %>
<div class="home-background">
  <div class="top-rated-carousel">
    <div class="home-tr-text">
      <h1 class="home-tr">Top rated:</h1>
      <h2 class="home-tr-sub">&nbsp;WINTER 2020</h2>
    </div>
    <% unless @top_rated.nil? %>
      <% @top_rated.each do |yacht| %>
        <%= render 'shared/yacht_card', yacht: yacht, margin: 20 %>
      <% end %>
    <% end %>
  </div>
  <div class="home-grey">

  </div>
</div>

